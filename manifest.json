{
  "manifest_version": 3,
  "name": "景灿-冲冲冲",
  "version": "1.0.0.1",
  "description": "在小红书、抖音、B站、快手等平台为PR提供约合帮助的工具",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting"
  ],
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.xiaohongshu.com/*",
        "*://*.xiaohongshu.cn/*",
        "*://*.sogou.com/*",
        "*://*.sogou.cn/*"
      ],
      "js": [
        "js/plugin/jquery.min.js",
        "js/xhs.js",
        "js/cardHtml.js",
        "js/douyin.js",
        "js/bili.js",
        "js/sogou.js",
        "js/myjs.js"
      ],
      "css": [
        "css/plugin/bootstrap.min.css",
        "css/mystyle.css"
      ]
    }
  ]
}